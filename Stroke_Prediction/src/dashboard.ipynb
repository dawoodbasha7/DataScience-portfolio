    "import streamlit as st\n",
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"healthcare-dataset-stroke-data.csv\")\n",
    "st.title(\"Stroke Dataset Explorer\")\n",
    "\n",
    "if st.checkbox(\"Show data\"):\n",
    "    st.dataframe(df)\n",
    "\n",
    "if st.checkbox(\"Show BMI distribution\"):\n",
    "    st.hist(df['bmi'].dropna(), bins=30)\n"
